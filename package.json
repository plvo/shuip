{
  "name": "shuip",
  "version": "1.1.0",
  "author": "plvo <plvdev@proton.me>",
  "license": "MIT",
  "description": "Ship fast with sh(ui)p, a collection of components and blocks for your Next.js project, built with shadcn/ui",
  "homepage": "https://github.com/plvo/shuip",
  "bugs": {
    "url": "https://github.com/plvo/shuip/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plvo/shuip"
  },
  "type": "module",
  "packageManager": "bun@1.2.10",
  "scripts": {
    "clean": "rimraf .next node_modules bun.lock package-lock.json",
    "check": "biome check --write .",
    "check:unused": "knip",
    "format": "biome format --write .",
    "lint": "biome lint --write .",
    "build": "next build --turbopack",
    "build:analyze": "ANALYZE=true next build",
    "build:full": "next build && bun run registry:generate && bun run registry:build",
    "dev": "next dev",
    "start": "next start",
    "registry:build": "shadcn build",
    "registry:generate": "bun run scripts/generate-registry.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/mdx": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/mdx": "^15.5.3",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/postcss": "^4.1.14",
    "@tanstack/react-query": "^5.90.2",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.545.0",
    "mdast-util-toc": "^7.1.0",
    "next": "15.5.3",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.4.6",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.64.0",
    "shadcn": "^3.4.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "husky": "^9.1.7",
    "knip": "^5.64.2",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3"
  }
}
